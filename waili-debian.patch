+void LiftChannelC::ILift_bin(int type ) const
+{
+  PixType *even = Even;
+  PixType *odd = Odd;
+  
+  for (u_int i = 0; i < Rodd; i++) {    
+    for (u_int j = 0; j < Cols; j++)
+      odd[j] = XOR(even[j], odd[j]) ;
+    even += Cols;
+    odd += Cols;
+  }
