+  case 101: //ID_SWE_13_7
+    {  static double g[2]={ 1.45, 2.2  }; return g;}
+  case 1://ID_CDF_1_1
+  case 2:// ID_CDF_1_
+  case 3://ID_CDF_1_5
+    {  static double g[2]={ 1.2, 2.0 };return g;}
+  case 4://ID_CDF_2_2 = 4
+  case 5://ID_CDF_2_4
+  case 6://ID_CDF_2_6
+    {  static double g[2]={ 1.5, 2.0 };return g;}
+  case 7://ID_CDF_4_2
